    <presentationTable>
      <presentation id="ooName">
        <text>Especifica el nombre del producto.</text>
        <textBox refId="Value">
          <label>Valor:</label>
          <defaultValue>Collabora Office</defaultValue>
        </textBox>
        <checkBox refId="Final">Final</checkBox>
      </presentation>
      <presentation id="UserDefinedSettings">
        <checkBox refId="Value">Cargar configuración específica del usuario con el documento</checkBox>
        <checkBox refId="Final">Final</checkBox>
      </presentation>
      <presentation id="UseUserData">
        <checkBox refId="Value">Utilizar datos de usuario en las propiedades del documento</checkBox>
        <checkBox refId="Final">Final</checkBox>
      </presentation>
      <presentation id="AutoSave">
        <checkBox refId="Value">Guardar información de recuperación automática</checkBox>
        <checkBox refId="Final">Final</checkBox>
        <text>Guardar información de recuperación automática cada __ minutos</text>
        <text>Tipo: entero [1..60]</text>
        <decimalTextBox refId="AutoSaveTimeIntervallValue" defaultValue="15" spin="true">Valor</decimalTextBox>
        <checkBox refId="AutoSaveTimeIntervallFinal">Final</checkBox>
        <checkBox refId="AutoSavePromptValue">También guardar documento automáticamente</checkBox>
        <checkBox refId="AutoSavePromptFinal">Final</checkBox>
      </presentation>
      <presentation id="RecoveryInfo">
        <checkBox refId="Value">Disable the autorecovery feature</checkBox>
        <checkBox refId="Final">Final</checkBox>
      </presentation>
      <presentation id="CreateBackup">
        <checkBox refId="Value">Crear siempre una copia de seguridad</checkBox>
        <checkBox refId="Final">Final</checkBox>
      </presentation>
      <presentation id="EditProperty">
        <checkBox refId="Value">Editar propiedades del documento antes de guardar</checkBox>
        <checkBox refId="Final">Final</checkBox>
      </presentation>
      <presentation id="WarnAlienFormat">
        <checkBox refId="Value">Alertar al usuario cuando se guarde en un formato distinto de ODF o del predeterminado</checkBox>
        <checkBox refId="Final">Final</checkBox>
      </presentation>
      <presentation id="AutoCorrect">
        <text>WritePath of AutoCorrect files. It is the location where new user files are written.</text>
        <textBox refId="WritePathValue">
          <label>Ruta:</label>
          <defaultValue>$(user)/autocorr</defaultValue>
        </textBox>
        <checkBox refId="WritePathFinal">Final</checkBox>
        <text>UserPaths of AutoCorrect files</text>
        <textBox refId="UserPathsValue">
          <label>Lista de rutas:</label>
          <defaultValue>$(inst)/share/autocorr $(user)/autocorr</defaultValue>
        </textBox>
        <checkBox refId="UserPathsFinal">Final</checkBox>
      </presentation>
      <presentation id="AutoText">
        <text>WritePath of AutoText files. It is the location where new user files are written.</text>
        <textBox refId="WritePathValue">
          <label>Ruta:</label>
          <defaultValue>$(user)/autotext</defaultValue>
        </textBox>
        <checkBox refId="WritePathFinal">Final</checkBox>
        <text>UserPaths of AutoText files</text>
        <textBox refId="UserPathsValue">
          <label>Lista de rutas:</label>
